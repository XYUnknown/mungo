aspect Annotations {
    syn Modifier Modifiers.getTypestateAnnotation() {
        for (Modifier m: this.getModifierList()) {
            if (m instanceof TypestateAnnotation) {
                return m;
            }
        }
        return null;
    }

    syn boolean Modifiers.hasTypestateAnnotation() {
        return getTypestateAnnotation() != null;
    }
}
