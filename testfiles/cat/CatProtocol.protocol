package examples.cat;

typestate CatProtocol {
	Init = {
		void initialise(): Alive
	}

	Alive = {
		void putInBox(): Schrodinger,
		void playWith(Cat<<Alive>>): Alive,
		void poison(): Dead,
		void close(): end
	}

	Dead = {
		void putInBox() : Schrodinger,
		void close(): end
	}

	Schrodinger = {
		void poison() : Dead,
		BooleanChoice openBox() : <TRUE: Alive, FALSE: Dead>
	}
}